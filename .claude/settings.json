{
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "permissions": {
    "allow": [
      "Bash(ls:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(tree:*)",
      "Bash(xcodebuild:*)",
      "Bash(xcsift:*)",
      "Bash(/opt/homebrew/bin/xcsift:*)",
      "Bash(xcbeautify:*)",
      "Bash(printf:*)",
      "Bash(osascript:*)",
      "Bash(git log:*)",
      "Bash(git diff:*)",
      "Bash(git status:*)",
      "Bash(git branch:*)",
      "Bash(git fetch:*)",
      "Bash(git stash:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git merge:*)",
      "Bash(git rm:*)",
      "Bash(git checkout:*)",
      "Bash(git ls-tree:*)",
      "Bash(gh pr view:*)",
      "Bash(gh pr list:*)",
      "Bash(gh pr diff:*)",
      "Bash(gh api:*)",
      "Bash(python3:*)",
      "Bash(swift:*)",
      "Bash(wc:*)",
      "Bash(echo:*)",
      "Skill",
      "mcp__*",
      "mcp__xcodebuildmcp__*",
      "mcp__XcodeBuildMCP__*",
      "mcp__plugin_context7_context7__*",
      "Search",
      "Read",
      "WebSearch"
    ],
    "defaultMode": "plan"
  },
  "hooks": {
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "bash ~/.claude/hooks/english-coach.sh",
            "statusMessage": "English coaching..."
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "bash -c '\"/opt/homebrew/bin/node\" \"$(ls -td ~/.claude/plugins/cache/claude-hud/claude-hud/*/ 2>/dev/null | head -1)dist/index.js\"'"
  },
  "enabledPlugins": {
    "swift-lsp@claude-plugins-official": true,
    "ralph-loop@claude-plugins-official": true,
    "superpowers@superpowers-marketplace": true,
    "playground@claude-plugins-official": true,
    "suggest-tidyings@corca-plugins": true,
    "claude-hud@claude-hud": true,
    "context7@claude-plugins-official": true,
    "session-wrap@team-attention-plugins": true,
    "swiftui-expert@swiftui-expert-skill": true,
    "xcsift@xcsift": true,
    "refactor@corca-plugins": true,
    "compound-engineering@every-marketplace": true
  },
  "language": "Korean"
}
