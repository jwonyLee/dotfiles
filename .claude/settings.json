{
  "permissions": {
    "allow": [
      "Bash(ls:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(tree:*)",
      "Bash(git log:*)",
      "Bash(git diff:*)",
      "Bash(xcodebuild:*)",
      "Bash(printf:*)",
      "Bash(osascript:*)",
      "Skill",
      "mcp__*",
      "mcp__xcodebuildmcp__*",
      "mcp__XcodeBuildMCP__*",
      "mcp__plugin_context7_context7__*",
      "Search",
      "Read",
      "WebSearch"
    ],
    "defaultMode": "plan"
  },
  "hooks": {
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "bash ~/.claude/hooks/english-coach.sh",
            "statusMessage": "English coaching..."
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "bash -c '\"/Users/admin/.bun/bin/bun\" \"$(ls -td ~/.claude/plugins/cache/claude-hud/claude-hud/*/ 2>/dev/null | head -1)src/index.ts\"'"
  },
  "enabledPlugins": {
    "swift-lsp@claude-plugins-official": true,
    "code-simplifier@claude-plugins-official": true,
    "code-review@claude-plugins-official": true,
    "oh-my-claude-sisyphus@oh-my-claude-sisyphus": true,
    "clarify@corca-plugins": true,
    "suggest-tidyings@corca-plugins": true,
    "claude-hud@claude-hud": true,
    "superpowers@superpowers-marketplace": true,
    "context7@claude-plugins-official": true,
    "session-wrap@team-attention-plugins": true,
    "oh-my-claudecode@oh-my-claude-sisyphus": true,
    "github@claude-plugins-official": true,
    "slack@claude-plugins-official": true,
    "skill-up@skill-up-marketplace": true,
    "superpowers@claude-plugins-official": true,
    "ralph-loop@claude-plugins-official": true,
    "document-skills@anthropic-agent-skills": true,
    "example-skills@anthropic-agent-skills": true,
    "swiftui-expert@swiftui-expert-skill": true,
    "playground@claude-plugins-official": true,
    "plannotator@plannotator": true
  },
  "language": "Korean"
}
